class LocationService {
  +{static}String ACTION_CAPTURE_LOCATIONS
  +{static}String ACTION_STOP_CAPTURIGN_LOCATIONS
  +{static}String ACTION_QUERY_STATUS_MAIN_ACTIVITY
  +{static}String ACTION_QUERY_STATUS_UPLOAD_SERVICE
